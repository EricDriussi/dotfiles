{
    "coc.preferences.formatOnSaveFiletypes": [
        "css",
        //"markdown",
        "javascript",
        "graphql",
        "html",
        "ejs",
        "yaml",
        "json",
        "python",
        "typescript",
        "java"
    ],
    // explorer
    "explorer.width": 30,
    "explorer.icon.enableNerdfont": true,
    "explorer.previewAction.onHover": false,
    "explorer.keyMappings.global": {
        "<cr>": ["expandable?", "expand", "open:tab"],
        "v": "open:vsplit"
    },
    "prettier.tabWidth": 4
}
